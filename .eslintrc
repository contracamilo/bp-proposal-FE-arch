{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"modules": true,
			"experimentalObjectRestSpread": true
		}
	},
	"env": {
		"browser": true,
		"node": true
	},
	"plugins": [
		"react"
	],
	"extends": "airbnb",
	"rules": {
		"indent": [
			2,
			2,
			{ "SwitchCase": 1 }
		],
		"no-tabs": 0,
		"comma-dangle": 0,
		"react/jsx-uses-vars": 1,
		"react/display-name": 1,
		"no-unused-vars": "warn",
		"no-console": 1,
		"no-unexpected-multiline": "warn",
		"react/prop-types": [
			"warn"
		],
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"linebreak-style": 0,
		"react/no-did-update-set-state": 0,
		"no-restricted-globals": ["error", "event", "fdescribe"],
		"react/destructuring-assignment": 0,
		"react/forbid-prop-types": 0,
		"import/no-named-as-default": 0,
		"no-shadow": 0,
		"jsx-a11y/label-has-for": [2, {
      "required": { "every": ["id"] }
    }]
	},
	"settings": {
		"react": {
			"pragma": "React",
			"version": "16.6.0"
		}
	},
	"globals": {
		"document": true,
		"foo": true,
		"window": true,
		"sessionStorage": true,
		"console": true,
		"fetch": true,
		"Promise": true,
		"process": true,
		"clearTimeout": true,
		"setTimeout": true,
		"Intl": true
	}
}